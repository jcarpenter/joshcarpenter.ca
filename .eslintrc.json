{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "guard-for-in": "off",
        "semi": ["error", "never"],
        "require-jsdoc": "off",
        "max-len": "off",
        "valid-jsdoc": "off",
        "no-invalid-this": "off",
        "no-unused-vars": "off",
        "object-curly-newline": "off"
    }
}