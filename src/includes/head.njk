<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>{{ title }} | Josh Carpenter</title>
<meta name="description" content="{{ summary }}">
<meta name="dcterms.date" content="{{ modified }}"/>
{# <meta name="author" content="{{ josh.name }}"/> #}
{# <meta name="copyright" content="Copyright Â© {{ date | date("YYYY") }}, {{ josh.name }}"/> #}

<!-- Social -->
<meta property="og:type" content="article">
<meta property="og:title" content="{{ title }} | Josh Carpenter">
<meta property="og:url" content="https://joshcarpenter.ca{{ page.url}}">
<meta property="og:description" content="{{ summary }}">
{% if image %}<meta property="og:image" content="{{ image }}">{% endif %}
<meta property="og:updated_time" content="{{ modified }}">
<meta property="article:modified_time" content="{{ modified }}">
<meta property="og:site_name" content="Josh Carpenter">
<meta property="og:locale" content="en_US">
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:site" content="@joshcarpenter">
<meta name="twitter:creator" content="@joshcarpenter">

